@model Przychodnia.Data.Entities.CennikPoradnia

@{
    ViewData["Title"] = "Cennik";
}

    <div class="row">

        <div class="col-lg-12">
            <div class="card shadow mb-4">
                <div class="card-header py-3">
                    <h6 class="m-0 font-weight-bold text-primary">Czy chcesz usunąć?</h6>
                </div>
                <div class="card-body">
                    <form asp-action="Delete">
                        <input type="hidden" asp-for="Id" />
                        <button type="submit" class="btn btn-primary">
                            <i class="fas fa-check"></i>
                        </button> <span></span>
                        <a class="btn btn-primary" asp-action="Index"><i class="fas fa-ban"></i> <span></span></a>
                    </form>
                </div>
            </div>
        </div>

    </div>
