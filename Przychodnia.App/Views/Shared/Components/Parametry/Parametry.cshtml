@model IEnumerable<Przychodnia.Data.Entities.Parametr>
<div class="container">
    <div class="row mb-5">
        @foreach (var item in Model.Where(p => p.Aktywny))
        {
            <div class="col-md-4">
                <div class="ftco-footer-widget mb-4">
                    <h2>@item.Wartosc</h2>
                    <p>@Html.Raw(item.Opis)</p>
                </div>
            </div>
        }
        @if (Model.Where(p => p.Aktywny).Count() % 3 == 0)
        {
            <div class="col-md-12 pr-md-4">
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2585.8452371719773!2d20.68925071578304!3d49.60066787936725!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x473de527c4c967d1%3A0x4656f90ca24683d9!2sWy%C5%BCsza%20Szko%C5%82a%20Biznesu%20-%20National%20Louis%20University!5e0!3m2!1spl!2spl!4v1636893903492!5m2!1spl!2spl"
                        width="100%" height="450" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
            </div>
        }
        @if (Model.Where(p => p.Aktywny).Count() % 3 == 1)
        {
            <div class="col-md-8 pr-md-4">
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2585.8452371719773!2d20.68925071578304!3d49.60066787936725!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x473de527c4c967d1%3A0x4656f90ca24683d9!2sWy%C5%BCsza%20Szko%C5%82a%20Biznesu%20-%20National%20Louis%20University!5e0!3m2!1spl!2spl!4v1636893903492!5m2!1spl!2spl"
                        width="100%" height="450" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
            </div>
        }
        @if (Model.Where(p => p.Aktywny).Count() % 3 == 2)
        {
            <div class="col-md-4 pr-md-4">
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2585.8452371719773!2d20.68925071578304!3d49.60066787936725!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x473de527c4c967d1%3A0x4656f90ca24683d9!2sWy%C5%BCsza%20Szko%C5%82a%20Biznesu%20-%20National%20Louis%20University!5e0!3m2!1spl!2spl!4v1636893903492!5m2!1spl!2spl"
                        width="100%" height="450" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
            </div>
        }
    </div>
</div>